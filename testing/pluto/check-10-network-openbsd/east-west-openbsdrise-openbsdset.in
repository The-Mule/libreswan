03-openbsdrise-ifconfig.sh

rise# ifconfig vio1
rise# ifconfig vio1 up
rise# ifconfig vio2
rise# ifconfig vio2 up

04-openbsdset-ifconfig.sh

set# ifconfig vio1
set# ifconfig vio1 up
set# ifconfig vio2
set# ifconfig vio2 up

05-openbsdrise-ping.sh

rise# ../../guestbin/ping-once.sh --up 198.18.1.15 # SET
rise# ../../guestbin/ping-once.sh --up 192.0.2.254  # EAST

06-openbsdset-ping.sh

set# ../../guestbin/ping-once.sh --up 198.18.1.12 # RISE
set# ../../guestbin/ping-once.sh --up 192.0.1.254  # WEST

