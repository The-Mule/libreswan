01-east-ifconfig.sh

east# ../../guestbin/ip.sh addr show eth0
east# ../../guestbin/ip.sh link set eth0 up
east# ../../guestbin/ip.sh addr show eth1
east# ../../guestbin/ip.sh link set eth1 up

02-west-ifconfig.sh

west# ../../guestbin/ip.sh addr show eth0
west# ../../guestbin/ip.sh link set eth0 up
west# ../../guestbin/ip.sh addr show eth1
