<network ipv6='yes'>
  <name>swandefault</name>
  <forward mode='nat'/>
  <bridge name='swandefault' stp='on' delay='0'/>
  <domain name='swandefault'/>
  <ip address='192.168.234.1' netmask='255.255.248.0'>
    <!-- 232 is 0xE8 or 0b_1110_1000 -->
    <!-- 234 is 0xEA or 0b_1110_1010 -->
    <!-- 239 is 0xEF or 0b_1110_1111 -->
    <!-- 239-334 = 6 * 256 = 1500 addresses -->
    <!-- 248 is 0xF8 or 0b_1111_1000 -->
    <dhcp>
      <range start='192.168.234.2' end='192.168.239.254'/>
    </dhcp>
  </ip>
</network>
